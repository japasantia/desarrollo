<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.collections.FXCollections?>
<?import ve.gob.cendit.cenditlab.ui.FieldInput?>

<fx:root fx:id="frequencyListPane" animated="false" text="Lista de frecuencias" type="TitledPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<!-- <TitledPane fx:id="frequencyListPane" animated="false" text="Lista de frecuencias" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"> -->
   <content>
      <VBox>
         <children>
            <ToolBar>
              <items>
                <Button fx:id="addButton" minWidth="50.0" mnemonicParsing="false" onMouseClicked="#addButtonClicked" text="+" />
                <Button fx:id="removeButton" minWidth="50.0" mnemonicParsing="false" onMouseClicked="#removeButtonClicked" text="-" />
              </items>
            </ToolBar>
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER">
               <content>
                  <ListView fx:id="frequencyListView" editable="true">
                  </ListView>
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </content>
<!-- </TitledPane> -->
</fx:root>
