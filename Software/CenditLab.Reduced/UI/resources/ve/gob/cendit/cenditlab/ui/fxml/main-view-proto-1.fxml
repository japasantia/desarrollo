<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<fx:root maxHeight="Infinity" maxWidth="Infinity" minHeight="400.0" minWidth="600.0" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<!-- <BorderPane maxHeight="Infinity" maxWidth="Infinity" minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"> -->
   <top>
      <VBox fx:id="topVBox" maxWidth="Infinity" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="headerHBox" maxWidth="Infinity">
               <children>
                  <ImageView fx:id="appLogoImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
                     <image>
                        <Image url="@../images/cendit-logo.jpg" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <VBox fx:id="toolbarVBox" maxWidth="Infinity" minHeight="40.0" />
         </children>
      </VBox>
   </top>
<!-- </BorderPane> -->
</fx:root>
