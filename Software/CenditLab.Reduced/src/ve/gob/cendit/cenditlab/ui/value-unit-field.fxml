<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>

<?scenebuilder-classpath-element .?>

<fx:root type="HBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<!-- <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"> -->
   <children>
      <TextField fx:id="valueTextField" minWidth="100.0" promptText="Introduzca valor" HBox.hgrow="SOMETIMES">
         <HBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </HBox.margin>
      </TextField>
      <ChoiceBox fx:id="unitsChoiceBox" HBox.hgrow="NEVER" value="Hz">
         <HBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </HBox.margin>
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Hz"/>
               <String fx:value="kHz"/>
               <String fx:value="MHz"/>
               <String fx:value="GHz"/>
            </FXCollections>
         </items>
      </ChoiceBox>
   </children>
<!-- </HBox> -->
</fx:root>
