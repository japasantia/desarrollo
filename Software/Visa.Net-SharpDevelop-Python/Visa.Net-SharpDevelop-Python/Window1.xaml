<?xml version="1.0" encoding="utf-8"?>
<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Visa.Net_SharpDevelop_Python"
	Height="300"
	Width="300">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="*" />
			<RowDefinition
				Height="Auto" />
		</Grid.RowDefinitions>
		<TextBox
			x:Name="outputTextBox"></TextBox>
		<Button
			x:Name="processButton"
			Grid.Row="1"
			Height="50"
			Click="processButton_Click">Run</Button>
	</Grid>
</Window>