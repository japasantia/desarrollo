<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="BOTTOM_LEFT" maxHeight="100.0" maxWidth="200.0" minHeight="80.0" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <ImageView fx:id="iconImageView" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@system.png" />
      </image>
      <HBox.margin>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" fx:id="x1" />
      </HBox.margin>
    </ImageView>
    <Label fx:id="nameLabel" text="Nombre del Componente" textAlignment="LEFT" textOverrun="CLIP" wrapText="true" HBox.hgrow="NEVER" HBox.margin="$x1">
      <font>
        <Font name="System Bold" size="14.0" />
      </font>
    </Label>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</HBox>
