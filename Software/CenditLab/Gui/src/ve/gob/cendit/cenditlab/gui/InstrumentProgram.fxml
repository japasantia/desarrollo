<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ve.gob.cendit.cenditlab.gui.InstrumentProgramController">
    <left>
        <VBox fx:id="leftPane" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="addButton" maxWidth="1.7976931348623157E308" onAction="#addButtonPressed" text="Add">Click me!</Button>
            <Accordion>
              <panes>
                    <TitledPane animated="false" text="Funciones básicas" />
                    <TitledPane animated="false" text="Programación" />
              </panes>
            </Accordion>
                <Button fx:id="returnMainWindowButton" maxWidth="1.7976931348623157E308" onAction="#returnMainWidowButtonPressed" text="Principal" />
            </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
        </VBox>
    </left>
   <center>
      <ScrollPane fx:id="scrollPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="controlsPane">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding></VBox>
         </content></ScrollPane>
   </center>
</BorderPane>
